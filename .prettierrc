{
  "semi": true,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "htmlWhitespaceSensitivity": "css",
  "printWidth": 150,
  "tailwindFunctions": ["clsx", "cn"],
  "tabWidth": 4,
  "plugins": ["prettier-plugin-blade", "prettier-plugin-organize-attributes"],
  "overrides": [
    {
      "files": [
        "*.blade.php"
      ],
      "options": {
        "parser": "blade"
      }
    }
  ]
}